{
    "name": "devianl2/tenant-support",
    "description": "Microservice support package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Devian Leong",
            "email": "devian@peoplelogy.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Tenant\\Support\\": "/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenant\\Support\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tenant\\Support\\TenantSupportProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
